{
  "name": "gar-keys",
  "displayName": "Gar's Keys",
  "description": "My favourite key shortcuts",
  "version": "0.0.2",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/devgar/gar-keys.git"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Keymaps"
  ],
  "scripts": {
    "major": "vsce publish major",
    "minor": "vsce publish minor",
    "patch": "vsce publish patch"
  },
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+n",
        "command": "explorer.newFile",
        "when": "filesExplorerFocus"
      },
      {
        "key": "ctrl+shift+e",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "filesExplorerFocus"
      },
      {
        "key": "ctrl+oem_1",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
      }
    ]
  },
  "author": {
    "name": "Edgar Albalate",
    "email": "dev.gardo@gmail.com",
    "url": "https://gar.im"
  },
  "devDependencies": {
    "@vscode/vsce": "2.15.0"
  },
  "publisher": "EAlbalate",
  "license": "MIT"
}
